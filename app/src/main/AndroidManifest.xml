<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sergei.com.superandroidproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LayoutActivity" />
        <activity android:name=".ButtonActivity" />
        <activity android:name=".TextActivity" />
        <activity android:name=".LayoutACTIVITYS.Linear" />
        <activity
            android:name=".LayoutACTIVITYS.Relative"
            android:label="@string/title_activity_relative"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LayoutACTIVITYS.Frame"
            android:label="@string/title_activity_frame"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>